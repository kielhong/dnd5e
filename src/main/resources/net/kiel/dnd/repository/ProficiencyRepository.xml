<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="net.kiel.dnd.repository.ProficiencyRepository">
    <select id="selectBonus" parameterType="int" resultType="int">
        SELECT bonus
        FROM   proficiency
        WHERE  level = #{level}
    </select>
</mapper>